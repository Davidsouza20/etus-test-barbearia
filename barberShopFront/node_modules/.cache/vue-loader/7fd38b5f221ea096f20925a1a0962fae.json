sses","sourcesContent":["<template> \n    <div>\n        <div class=\"class-navigation\">\n            <div class=\"embed-responsive embed-responsive-16by9\">\n                <iframe class=\"embed-responsive-item\" :src=\"video_path\" allow=\"accelerometer; encrypted-media; gyroscope; picture-in-picture\" allowfullscreen='' frameborder='0' title=\"Teste\"></iframe>\n            </div>\n            \n            <aside>\n                <header><strong>Aulas</strong></header>\n                <a v-for =\"aula in videos.items\" \n                    :key=\"aula.id\" \n                    class=\"list-group-item list-group-item-action flex\" \n                    @click=\"selectVideo(aula.contentDetails.videoId, aula.snippet.title, aula.snippet.description)\"\n                >\n                    <div class=\"thumbnail\"><img :src=\"aula.snippet.thumbnails.default.url\" alt=\"\"> </div>\n                    <span>{{ aula.snippet.title }}</span>\n                </a>\n                \n                <br> \n                <h5>Próximas Aulas <strong>Em Breve!</strong></h5>\n                <p>Fica de olho no seu Email!</p>\n            </aside>\n        </div>\n\n        <br>\n        \n        <div class=\"like\"> \n            <h5><strong>Clique em </strong> \n                    <div class=\"fb-like\" data-href=\"https://www.facebook.com/hellenfitness2/\" data-width=\"\" data-layout=\"button_count\" data-action=\"like\" data-size=\"large\" data-share=\"false\"></div>\n\n            e seja notificada no Facebook quando eu postar mais videos\n                \n            </h5>    \n        </div>\n\n        <hr>\n        <h4>QUERO OUVIR DE VOCÊ</h4>\n        <br>\n       <div \n        class=\"fb-comments\" \n        data-href=\"https://www.hellenfitness.com.br\" data-numposts=\"5\" \n        data-width=\"\"></div>\n    </div>   \n</template>\n\n<script>\nimport api from \"../../services/api\"\nimport axios from \"axios\"\n\nexport default {\n     data () {\n        return {\n            videos: [],\n            video_path: '',\n            title: '',\n            description: '',\n        }  \n    },\n\n    methods: {\n        selectVideo(video, class_title, description) {\n            this.video_path = 'https://www.youtube.com/embed/' + video + '?autohide=2&amp;showinfo=0&amp;rel=0&amp;modestbranding=1&amp;color=white';\n            this.title = class_title;\n            this.description = description;\n        },\n\n        FB_Parse () {\n            setTimeout (() => {\n                window.FB.XFBML.parse();\n            }, 0)\n        }\n    },\n\n    async created () {\n        // await api.get(`/aulas/category/${this.$route.params.category}`)\n        // .then(res =>  (this.classes = res.data))\n        // .catch(err => console.log(err));\n\n        // if (this.$route.params.category === 'PERNAS STRONG') {\n        //     this.video_path = 'https://www.youtube.com/embed/tzgcpl75Lck?autohide=2&amp;showinfo=0&amp;rel=0&amp;modestbranding=1&amp;color=white';\n        // }\n\n        // if (this.$route.params.category == 'HIT') {\n        //     this.video_path = 'https://www.youtube.com/embed/M84QZWQ6PUw?autohide=2&amp;showinfo=0&amp;rel=0&amp;modestbranding=1&amp;color=white';\n        // }\n\n        // if (this.$route.params.category === 'BRAÇOS E SUPERIORES') {\n        //     this.video_path = 'https://www.youtube.com/embed/fIC0UABbWgs?autohide=2&amp;showinfo=0&amp;rel=0&amp;modestbranding=1&amp;color=white';\n        // }\n\n        // if (this.$route.params.category === 'DANCE') {\n        //     this.video_path = 'https://www.youtube.com/embed/Cp3-ewui4DA?autohide=2&amp;showinfo=0&amp;rel=0&amp;modestbranding=1&amp;color=white';\n        // }\n\n\n        await axios.get(\"https://www.googleapis.com/youtube/v3/playlistItems?\" +\n        \"&playlistId=PLqvY5xEMeSrnzDxDEzFHXQFLClDppF9Ll\" +\n        \"&part=contentDetails,snippet\" +\n        \"&maxResults=50\" +\n        \"&key=AIzaSyBMf3Er0R5v8gooduyDFdCPLbEBX